{
  "name": "vtron-plus",
  "version": "0.0.0",
  "files": [
    "distlib",
    "distlib/*.css"
  ],
  "main": "distlib/vtron-plus.mjs",
  "module": "distlib/vtron-plus.mjs",
  "types": "distlib/src/packages/plug.d.ts",
  "style": "distlib/style.css",
  "exports": {
    ".": {
      "import": "./distlib/vtron-plus.mjs",
      "require": "./distlib/vtron-plus.umd.js"
    },
    "./distlib/style.css": {
      "import": "./distlib/style.css",
      "require": "./distlib/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "build-lib-tsc": "vue-tsc --noEmit && vite build --config vitelib.config.ts",
    "build-lib": "vite build --config vitelib.config.ts"
  },
  "dependencies": {
    "vite-plugin-dts": "^2.0.2",
    "vtron": "^0.1.9",
    "vue": "^3.2.45",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  }
}
